      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Calculator"
      mc:Ignorable="d" 
      d:DesignHeight="450"
      Title="history" Width="405.222">

    <Grid Margin="0,0,-0.4,-190.4" Background="#FF5F5B5B">
        <ScrollViewer VerticalScrollBarVisibility="Visible">
            <ListView x:Name="list_Results" Height="614" Width="382" BorderThickness="0" Background="#FF5F5B5B" ClipToBounds="True" SelectionChanged="ListView_SelectionChanged">
                <ListView.View>
                    <GridView>
                        <GridViewColumn/>
